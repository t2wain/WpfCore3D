<Window x:Class="WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp"
        xmlns:lib="clr-namespace:WpfApp.Lib3D;assembly=WpfApp.Lib3D"
        mc:Ignorable="d"
        Title="Learn 3D with HelixToolkit" Height="450" Width="800" WindowState="Maximized">
    <DockPanel LastChildFill="True">
        <lib:UMenu DockPanel.Dock="Top"></lib:UMenu>
        <lib:UViewPort x:Name="vp"></lib:UViewPort>
    </DockPanel>
</Window>
